{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_MODE": "production",
    "VITE_API_URL_PROD": "https://hunspelllive-production.up.railway.app"
  }
} 