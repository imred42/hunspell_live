{
  "name": "hunspell_live_frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node scripts/install-rollup.js && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:analyze": "source-map-explorer 'build/static/js/*.js'",
    "build:prod": "GENERATE_SOURCEMAP=false vite build",
    "build:optimize": "npm run build:prod && npm run build:analyze",
    "deps:check": "npm-check -u",
    "typecheck": "tsc --noEmit",
    "test": "echo \"No tests specified\" && exit 0",
    "vercel-build": "node scripts/install-rollup.js && tsc -b && vite build"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.10.3",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@formatjs/intl-locale": "^4.2.8",
    "antd": "^5.22.1",
    "axios": "^1.7.7",
    "bootstrap": "^5.3.0",
    "framer-motion": "^11.11.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.26.2",
    "react-select": "^5.9.0",
    "react-toastify": "^10.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^8.56.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.17",
    "csstype": "^3.1.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "npm-check": "^6.0.1",
    "postcss": "^8.4.31",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^4.4.2",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8"
  }
}
